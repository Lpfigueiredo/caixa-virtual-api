{
  "name": "vcaixa.dev",
  "version": "1.9.6",
  "description": "Aplicação para o processo seletivo da TecnoSpeed",
  "author": "Leonardo Paulo Figueiredo",
  "license": "ISC",
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker-compose up -f docker-development.yml",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:staged": "npm test -- --findRelatedTests",
    "test:verbose": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "check": "npm-check -s -u"
  },
  "keywords": [
    "workspace",
    "typescript",
    "node",
    "eslint"
  ],
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.2",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.5.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.4.4",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "lint-staged": "^10.2.11",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "sucrase": "^3.15.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "celebrate": "^12.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.0",
    "nodemon": "^2.0.4",
    "swagger-ui-express": "^4.1.4"
  },
  "engines": {
    "node": "12.x"
  }
}
