{
  "name": "my-node-ts-workspace",
  "version": "1.0.0",
  "description": "My TypeScrypt Node Workspace",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:staged": "npm test -- --findRelatedTests",
    "test:verbose": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "check": "npm-check -s -u"
  },
  "keywords": [
    "workspace",
    "typescript",
    "node",
    "eslint"
  ],
  "author": "Leonardo Paulo Figueiredo",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.2",
    "@types/jest": "^26.0.9",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.4.4",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "lint-staged": "^10.2.11",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.0"
  }
}
